{
  "name": "@okkema/worker",
  "version": "2.0.0",
  "description": "Cloudflare Worker Web Framework",
  "files": [
    "dist"
  ],
  "module": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "sentry": [
        "./dist/sentry/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/core/index.js",
    "./utils": "./dist/utils/index.js",
    "./sentry": "./dist/sentry/index.js"
  },
  "scripts": {
    "build": "npm-run-all build:babel build:tsc",
    "build:babel": "babel ./src -d ./dist -x .ts --env-name production",
    "build:tsc": "tsc -b tsconfig.build.json",
    "clean": "rm -rf ./dist",
    "lint": "eslint '**/*.{js,ts,json}'",
    "lint:fix": "eslint '**/*.{js,ts,json}' --fix",
    "postpublish": "npm run clean",
    "prepack": "npm-run-all clean lint build test",
    "prepare": "husky install",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:verbose": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okkema/worker.git"
  },
  "keywords": [
    "okkema",
    "worker",
    "cloudflare"
  ],
  "author": "Okkema Labs <correos@okkema.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okkema/worker/issues"
  },
  "homepage": "https://github.com/okkema/worker#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@cloudflare/workers-types": "^2.2.2",
    "@peculiar/webcrypto": "^1.1.7",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/node-jose": "^1.1.8",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "jest": "^27.0.1",
    "lint-staged": "^11.1.2",
    "node-jose": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "service-worker-mock": "^2.0.5",
    "typescript": "^4.3.2"
  }
}
